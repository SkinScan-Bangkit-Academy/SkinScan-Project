<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintVertical_bias="0.486"
            android:layout_height="30dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.494"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="320dp"
            android:id="@+id/onboardingword1" />
        <Constraint
            android:layout_height="15dp"
            motion:layout_constraintEnd_toStartOf="@id/menu2"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="248dp"
            motion:layout_constraintHorizontal_bias="0.5"
            android:layout_width="15dp"
            motion:layout_constraintTop_toBottomOf="@id/onboardingword5"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:id="@+id/menu1" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:id="@+id/menu2"
            motion:layout_constraintStart_toEndOf="@id/menu1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="15dp"
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="@id/menu1"
            motion:layout_constraintTop_toTopOf="@id/menu1"
            motion:layout_constraintHorizontal_bias="0.5" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:layout_height="191dp"
            motion:layout_constraintEnd_toEndOf="@+id/logoMenu2"
            android:layout_width="225dp"
            motion:layout_constraintTop_toBottomOf="@+id/logoMenu2"
            android:id="@+id/descMenu2"
            android:layout_marginTop="32dp"
            motion:layout_constraintStart_toStartOf="@+id/logoMenu2"
            android:layout_marginStart="60dp"
            motion:layout_editor_absoluteX="554dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_constraintVertical_bias="0.448"
            android:layout_height="30dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.494"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="320dp"
            android:id="@+id/onboardingword1" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/menu2" >
        <Constraint
            android:layout_height="30dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="320dp"
            android:id="@+id/onboardingword1"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/onboardingword1"
            android:id="@+id/onboardingword2"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/onboardingword4"
            android:id="@+id/onboardingword5"
            motion:layout_constraintEnd_toStartOf="parent" />
        <Constraint
            android:layout_height="200dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="200dp"
            motion:layout_constraintVertical_bias="0.3"
            android:id="@+id/logoMenu1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/onboardingword3"
            motion:layout_constraintEnd_toEndOf="@+id/onboardingword2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@id/onboardingword5"
            android:layout_marginTop="4dp"
            motion:layout_constraintTop_toBottomOf="@id/onboardingword2"
            android:alpha="100"
            motion:layout_constraintHorizontal_bias="0.686" />
        <Constraint
            android:id="@+id/onboardingword4"
            motion:layout_constraintEnd_toEndOf="@id/onboardingword5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/onboardingword2"
            android:layout_marginTop="4dp"
            motion:layout_constraintTop_toBottomOf="@id/onboardingword2"
            android:alpha="100"
            motion:layout_constraintHorizontal_bias="0.27" />

        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/logoMenu1"
            android:layout_marginTop="16dp"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="@+id/logoMenu1"
            android:layout_width="0dp"
            motion:layout_constraintTop_toBottomOf="@+id/logoMenu1"
            android:id="@+id/titleMenu1"/>
        <Constraint
            android:layout_height="200dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="200dp"
            motion:layout_constraintVertical_bias="0.285"
            android:id="@+id/logoMenu2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.497" />
        <Constraint
            android:layout_height="67dp"
            motion:layout_constraintEnd_toEndOf="@id/descMenu2"
            android:layout_width="308dp"
            android:id="@+id/slideToAct"
            motion:layout_constraintStart_toStartOf="@+id/descMenu2"
            motion:layout_editor_absoluteY="599dp"
            motion:layout_editor_absoluteX="70dp" />
        <Constraint
            android:layout_width="227dp"
            android:layout_height="104dp"
            motion:layout_constraintTop_toBottomOf="@+id/logoMenu2"
            android:layout_marginTop="8dp"
            android:id="@+id/descMenu2"
            motion:layout_constraintEnd_toEndOf="@+id/logoMenu2"
            motion:layout_constraintStart_toStartOf="@+id/logoMenu2"
            motion:layout_editor_absoluteX="127dp" />
        <Constraint
            android:layout_height="15dp"
            motion:layout_constraintEnd_toStartOf="@id/menu2"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="240dp"
            motion:layout_constraintHorizontal_bias="0.5"
            android:layout_width="15dp"
            motion:layout_constraintTop_toBottomOf="@id/onboardingword5"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:id="@+id/menu1" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:id="@+id/menu2"
            motion:layout_constraintStart_toEndOf="@id/menu1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="15dp"
            android:layout_height="15dp"
            motion:layout_constraintBottom_toBottomOf="@id/menu1"
            motion:layout_constraintTop_toTopOf="@id/menu1"
            motion:layout_constraintHorizontal_bias="0.5" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/menu2" >
        <OnSwipe motion:dragDirection="dragLeft" />
    </Transition>

</MotionScene>